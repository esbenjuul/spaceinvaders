mp to fixed framerate\n    let now = Math.round((this.fps * Date.now()) / 1000);\n    if (now === prevTick) return;\n    prevTick = now;\n    this.update();\n    this.draw();\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"12314e92359cbcf03558b4987dffa79b","cacheData":{"env":{}}}